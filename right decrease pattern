class HelloWorld {
public static void main(String[] args) {
    System.out.println("right decreasing pattern");  
        int m=5;
        for(int i=1; i<=m; i++) {          
          for(int j=4; j>=i;j--){
                System.out.print("*");
            }
            System.out.println();
        }

      }
    }
